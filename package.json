{
  "name": "express-typescript-api",
  "version": "1.0.0",
  "description": "Express Typescript API",
  "main": "index.js",
  "scripts": {
    "up": "docker-compose up",
    "down": "docker-compose down",
    "dev": "nodemon --watch src --ext 'ts' --exec 'node' -r tsconfig-paths/register -r ts-node/register src/index.ts",
    "build": "tsc --build",
    "start": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --fix --ext .ts",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
        "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
        "yarn fix"
    ]
  },
  "author": "Stuart Haas",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "pg": "^8.7.1",
    "tsyringe": "^4.6.0",
    "typeorm": "0.2.38",
    "reflect-metadata": "^0.1.10",
    "mysql": "^2.14.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "3.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.3.3333"
  }
}